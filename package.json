{
    "name": "fig-landing",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "@craco/craco": "^7.1.0",
        "i18next": "^24.0.5",
        "i18next-browser-languagedetector": "^8.0.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-i18next": "^15.1.3",
        "react-router-dom": "^7.0.2",
        "react-scripts": "5.0.1",
        "sass": "^1.82.0",
        "styled-components": "^6.1.13",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "craco start",
        "build": "GENERATE_SOURCEMAP=false craco build && npm run write-version",
        "format": "prettier --write \"**/*.{js,jsx,mjs,ts,tsx,mdx}\" --cache",
        "write-version": "git log -1 --pretty=format:\"Hash: %H%nDate: %ad%nMessage: %s\" > build/version.txt",
        "deploy-testnet": "aws s3 sync ./build s3://tech.firmainnovation.com --profile=firmachain-deploy --delete"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
        "@types/node": "^16.7.13",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "path": "^0.12.7",
        "prettier": "^3.4.2",
        "process": "^0.11.10",
        "typescript": "^5.7.2",
        "webpack": "^5.97.0"
    }
}
