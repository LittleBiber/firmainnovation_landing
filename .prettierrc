{
  "bracketSpacing": true,
  "printWidth": 140,
  "singleQuote": true,
  "trailingComma": "none",
  "tabWidth": 4,
  "useTabs": false,
  "importOrder": [
    "^node:$",
    "",
    "^(react/(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/types$",
    "^@/types/(.*)$",
    "^@/config$",
    "^@/config/(.*)$",
    "^@/paths$",
    "^@/data/(.*)$",
    "^@/lib/(.*)$",
    "^@/actions/(.*)$",
    "^@/contexts/(.*)$",
    "^@/hooks/(.*)$",
    "^@/components/(.*)$",
    "^@/styles/(.*)$",
    "",
    "^[./]"
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
